{
    "extends": "airbnb-base",
    "rules": {
      "one-var": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-dynamic-require": 0,
      "no-console": 0,
      "func-names": 0,
      "no-param-reassign": 0,
      "max-len": 0,
      "no-plusplus": 0,
      "no-buffer-constructor": 0,
      "global-require": 0,
      "class-methods-use-this": 0
  }
}
